<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/color_77f33e25" />
            <corners android:radius="@dimen/bg_line_corners" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <stroke android:width="@dimen/bg_line_width" android:color="@color/color_f33e25" />
            <corners android:radius="@dimen/bg_line_corners" />
        </shape>
    </item>
</selector>