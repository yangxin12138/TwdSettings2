<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/color_77f33e25" />
            <corners android:radius="@dimen/item_lr_line_corners" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <stroke android:width="@dimen/item_lr_line_width" android:color="@color/color_f33e25" />
            <corners android:radius="@dimen/item_lr_line_corners" />
        </shape>
    </item>
    <item android:state_focused="false" android:state_enabled="true">
        <shape>
            <stroke android:width="@dimen/item_lr_line_width" android:color="@color/color_ff000000" />
            <corners android:radius="@dimen/item_lr_line_corners" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <stroke android:width="@dimen/item_lr_line_width" android:color="@color/color_999999" />
            <corners android:radius="@dimen/item_lr_line_corners" />
        </shape>
    </item>
</selector>